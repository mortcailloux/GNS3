import ipaddress

# Votre adresse IPv6
adresse = "2001:168:192::2/64"

# Créer un objet IPv6Network
network = ipaddress.IPv6Network(adresse, strict=False)

# Extraire l'adresse IPv6 et le préfixe
adresse_ipv6 = str(network.network_address)
prefixe = network.prefixlen

# Afficher le résultat
print(f"Adresse IPv6 : {adresse_ipv6}/{prefixe}")